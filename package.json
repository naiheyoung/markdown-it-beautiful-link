{
  "name": "markdown-it-beautiful-link",
  "version": "0.0.2",
  "packageManager": "pnpm@10.28.0",
  "description": "link plugin for markdown-it.",
  "homepage": "https://github.com/naiheyoung/markdown-it-beautiful-link#readme",
  "author": "Naihe",
  "repository": {
    "type": "git",
    "url": "https://github.com/naiheyoung/markdown-it-beautiful-link.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./style": "./dist/index.css"
  },
  "scripts": {
    "test": "vitest",
    "prebuild": "pnpm format",
    "build": "tsdown",
    "format": "prettier --write src/",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "markdown-it-beautiful-link"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.9",
    "markdown-it": "^14.1.0",
    "prettier": "^3.8.0",
    "tsdown": "0.20.0-beta.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "markdown-it": "^14.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}